@{
}
@model List<Vision.Models.Skill>

@{
    if (Model.Count > 0)
    {
        <div class="card">
            <div class="card-body">
                <h5 class="card-title mb-4">Skills</h5>
                <div class="d-flex flex-wrap gap-2 fs-15">
@{
    foreach (var item in Model)
                        {
                          <a href="javascript:void(0);"
                   class="badge badge-soft-primary">@item.SkillTitle</a>   
                        }
}                   
                   
                </div>
            </div><!-- end card body -->
        </div>

        <!-- end card -->
    }
}
